        "resizeMode": "contain"
      },
      "image": "./assets/heimApo/splash-icon.png",
      "jest": {
        "preset": "jest-expo"
      },
      "plugins": [
        "healthkit-writer"
      ],
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "light",
    "version": "1.0.0"
